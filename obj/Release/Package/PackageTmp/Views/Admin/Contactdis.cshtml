@model List<CHC1.edmx.tblContactmemb>

@{
    ViewBag.Title = "Contactdis";
    
}

<h2>Contactdis</h2>

<body>

    <table class="table table-bordered table-hover table-responsive table-striped table-condensed">
        <tr>
            <th>MemberID</th>
            <th>FullName</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Message</th>

        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>@item.MemberID</td>
                <td>@item.FullName</td>
                <td>@item.Email</td>
                <td>@item.Phone</td>
                <td>@item.Message</td>


            </tr>
        }

    </table>
</body>