@model List<CHC1.edmx.tblPrgmRegistration>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>
<body>

    <table class="table table-bordered table-hover table-responsive table-striped table-condensed">
        <tr>
            <th>RegistrationID</th>
            <th>Name</th>
            <th>Email</th>
            <th>Phone</th>
            <th>AgeGroup</th>
            <th>Country</th>
            <th>City</th>
            <th>Message</th>
            <th>DateCreated</th>


        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>@item.RegistrationID</td>
                <td>@item.Name</td>
                <td>@item.Email</td>
                <td>@item.Phone</td>
                <td>@item.AgeGroup</td>
                <td>@item.Country</td>
                <td>@item.City</td>
                <td>@item.Message</td>
                <td>@item.DateCreated</td>


            </tr>
        }

    </table>
</body>